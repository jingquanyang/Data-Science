   "source": [
    "#Identify outliers in numeric columns\n",
    "numeric_cols = ['log_views', 'log_followers', 'duration']\n",
    "\n",
    "for col in numeric_cols:\n",
    "    Q1 = df[col].quantile(0.25)\n",
    "    Q3 = df[col].quantile(0.75)\n",
    "    IQR = Q3 - Q1\n",
    "    outliers = df[(df[col] < Q1 - 1.5*IQR) | (df[col] > Q3 + 1.5*IQR)]\n",
    "    print(f\"{col} has {len(outliers)} outliers\")"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 112,
   "id": "658038c6-7b7e-47d3-9d60-d36be50a0f52",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "Number of rows with z-score > 3: 5\n"
     ]
    }
   ],
   "source": [
    "from scipy import stats\n",
    "import numpy as np\n",
    "\n",
    "z_scores = np.abs(stats.zscore(df[numeric_cols]))\n",
    "outliers = (z_scores > 3).any(axis=1)\n",
    "print(f\"Number of rows with z-score > 3: {outliers.sum()}\")"
   ]
  },
